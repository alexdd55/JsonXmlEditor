{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "test": "npm run test:ui",
    "test:ui": "rm -rf .tmp-tests && npx tsc --target ES2022 --module NodeNext --moduleResolution NodeNext --outDir .tmp-tests src/lib/tabState.ts src/lib/toolbarState.ts src/lib/appI18n.ts src/lib/appUtils.ts && node --test ui-tests/*.test.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tsconfig/svelte": "^5.0.7",
    "svelte": "^5.51.2",
    "svelte-check": "^4.4.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "monaco-editor": "^0.55.1"
  }
}
